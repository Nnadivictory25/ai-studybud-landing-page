		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"postcss": "^8.4.38",
		"postcss-load-config": "^5.1.0",
		"prettier": "^3.2.5",
